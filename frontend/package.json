{
  "name": "btcfix-fe",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@getalby/lightning-tools": "^4.0.2",
    "@holepunch/light-bolt11-decoder": "^2.1.1",
    "emotion-reset": "^3.0.1",
    "graphql": "^16.8.0",
    "graphql-ws": "^5.14.0",
    "isbot": "^3.6.13",
    "next-share": "^0.25.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-qr-code": "^2.0.12",
    "react-router-dom": "^6.15.0",
    "react-select": "^5.7.4",
    "react-share": "^4.4.1",
    "use-resize-observer": "^9.1.0"
  },
  "scripts": {
    "start:dev": "VITE_APP_STAGE=dev vite",
    "start:test": "vite preview --outDir dist-test --port 3000",
    "build:deploy": "[ -z \"$STAGE\" ] && echo \"STAGE is not set, cannot proceed with build.\" && exit 1 || echo \"Building for stage: ${STAGE}\" && export VITE_APP_STAGE=$STAGE && tsc && vite build --outDir dist",
    "build:test": "export VITE_APP_STAGE=dev tsc && vite build --outDir dist-test",
    "test": "jest --all --testPathIgnorePatterns '.*.e2e.test.ts'",
    "test:e2e": "concurrently --success last --kill-others \"npm run start:test\" \"jest --all --env node --testMatch \\\"**/?(*.)+(e2e.test).[jt]s?(x)\\\"\"",
    "test:clear": "rm -rf src/test/__image_snapshots__",
    "generate": "source .env && echo $VITE_APP_API_URL_HTTP_DEV && export SCHEMA=${VITE_APP_API_URL_HTTP_DEV} && npx graphql-codegen",
    "tree": "tree -I 'node_modules|dist|dist-test' -P '*.tsx|*.ts|*.css'",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not iOS < 15",
      "not chrome < 80"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@emotion/jest": "^11.11.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@silvermine/serverless-plugin-cloudfront-lambda-edge": "^2.2.3",
    "@swc/core": "^1.3.80",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "@types/jest-image-snapshot": "^6.2.0",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@webbtc/webln-types": "^2.0.0",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "eslint-config-current-thing": "^1.3.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-image-snapshot": "^6.2.0",
    "puppeteer": "^21.1.0",
    "puppeteer-core": "^21.1.0",
    "serverless": "^3.34.0",
    "serverless-lift": "^1.27.0",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-s3-sync": "^3.1.0",
    "serverless-scriptable-plugin": "^1.3.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
